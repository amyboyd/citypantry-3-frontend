{
  "name": "citypantry-3-frontend",
  "title": "CityPantry",
  "scripts": {
    "postinstall": "npm run webdriverupdate && grunt copy:dist sass csso js",
    "test": "npm run postinstall && vagrant up && ./bin/wait-for-dev && ./bin/load-db-fixtures && ./node_modules/protractor/bin/protractor etc/protractor.js; vagrant halt;",
    "test-amy": "bin/load-db-fixtures && LOCAL_SELENIUM=1 node_modules/protractor/bin/protractor etc/protractor.js",
    "test-no-halt": "npm run postinstall && vagrant up && ./bin/wait-for-dev && ./bin/load-db-fixtures && ./node_modules/protractor/bin/protractor etc/protractor.js;",
    "test-local": "/home/citypantry/project/api/app/console doctrine:mongodb:fixtures:load && LOCAL_SELENIUM=1 ./node_modules/protractor/bin/protractor etc/protractor.js;",
    "update-webdriver": "node_modules/protractor/bin/webdriver-manager update",
    "link-chromedriver": "ln -s /home/citypantry/project/frontend/node_modules/chromedriver/bin/chromedriver node_modules/protractor/selenium/chromedriver.exe",
    "install-pre-commit": "ln -s ../../bin/pre-commit .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit"
  },
  "url": "https://citypantry.com/",
  "copyright": "2015",
  "version": "3.0.0",
  "dependencies": {},
  "devDependencies": {
    "chromedriver": "~2.9.1-1",
    "grunt": "~0.4.1",
    "grunt-6to5": "~3.0.0",
    "grunt-contrib-concat": "~0.5.0",
    "grunt-contrib-copy": "~0.7.0",
    "grunt-contrib-imagemin": "~0.7.1",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-csso": "git://github.com/amyboyd/grunt-csso.git",
    "grunt-sass": "~0.16.1",
    "jasmine-reporters": "~0.4.0",
    "jshint": "~2.6.0",
    "matchdep": "~0.1.2",
    "protractor": "~1.0.0",
    "reattempt-promise-function": "0.0.6",
    "request-promise": "^0.4.0",
    "selenium-webdriver": "~2.x"
  }
}
